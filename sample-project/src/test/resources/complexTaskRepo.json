{
  "plugins": [
    {"name": "AddTwoNumber", "className": "io.github.manhnt217.task.sample.plugin.AddTwoNumber"},
    {"name": "Curl", "className": "io.github.manhnt217.task.sample.plugin.Curl"},
    {"name": "Log", "className": "io.github.manhnt217.task.sample.plugin.Log"},
    {"name": "ObjectRefConsumer", "className": "io.github.manhnt217.task.sample.plugin.ObjectRefConsumer"},
    {"name": "ObjectRefProducer", "className": "io.github.manhnt217.task.sample.plugin.ObjectRefProducer"}
  ],
  "functions": [
    {
      "name": "c1",
      "outputMapping": ".Add",
      "group": {
        "activities": [
          {
            "name": "Add",
            "type": "PLUGIN",
            "inputMapping": ".START",
            "pluginName": "AddTwoNumber"
          },
          {
            "name": "Log",
            "type": "PLUGIN",
            "inputMapping": "{\"message\":\"The result of Add task is: \" + .Add, \"severity\": \"INFO\"}",
            "pluginName": "Log"
          }
        ],
        "links": [
          {
            "from": "START",
            "to": "Add"
          },
          {
            "from": "Add",
            "to": "Log",
            "guard": ".Add < 100"
          },
          {
            "from": "Add",
            "to": "END",
            "guard": "<otherwise>"
          },
          {
            "from": "Log",
            "to": "END"
          }
        ]
      }
    }
  ]
}